# Example environment variables for Mail-Jack
# Copy to `.env` and edit values for local development

# HTTP server
PORT=8080

# Provider
# Currently supported: SES
EMAIL_PROVIDER=SES

# AWS (required when using SES provider)
# AWS credentials can also be supplied by IAM role or shared config
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_SESSION_TOKEN=

# Postgres (used by the repository to persist email logs)
DB_HOST=localhost
DB_PORT=5432
DB_USER=mailjack
DB_PASSWORD=secret
DB_NAME=mailjack
# API Authentication
MAIL_JACK_API_KEY=your_secret_api_key_here
